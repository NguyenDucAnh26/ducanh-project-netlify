 setProvinceID(value);\\r\\n                          }}\\r\\n                        >\\r\\n                          {renderProvince}\\r\\n                        </Select>\\r\\n                      </Form.Item>\\r\\n                    </S.FlexItemSelect>\\r\\n                    <S.FlexItemSelect>\\r\\n                      <Form.Item\\r\\n                        rules={[\\r\\n                          {\\r\\n                            required: true,\\r\\n                            message: \\\"vui lòng chọn quận/huyện!\\\",\\r\\n                          },\\r\\n                        ]}\\r\\n                        name=\\\"district\\\"\\r\\n                      >\\r\\n                        {userInfo.data.district ? (\\r\\n                          <Select\\r\\n                            onChange={(value) => {\\r\\n                              setDistrictID(value);\\r\\n                            }}\\r\\n                            placeholder=\\\"Quận/huyện\\\"\\r\\n                            className=\\\"select-payment-cart\\\"\\r\\n                          >\\r\\n                            {renderDistrict}\\r\\n                          </Select>\\r\\n                        ) : (\\r\\n                          <Select\\r\\n                            onChange={(value) => {\\r\\n                              setDistrictID(value);\\r\\n                            }}\\r\\n                            placeholder=\\\"Quận/huyện\\\"\\r\\n                            className=\\\"select-payment-cart\\\"\\r\\n                          >\\r\\n                            {renderDistrict}\\r\\n                          </Select>\\r\\n                        )}\\r\\n                      </Form.Item>\\r\\n                    </S.FlexItemSelect>\\r\\n                    <S.FlexItemSelect>\\r\\n                      <Form.Item\\r\\n                        rules={[\\r\\n                          {\\r\\n                            required: true,\\r\\n                            message: \\\"vui lòng chọn phường/xã!\\\",\\r\\n                          },\\r\\n                        ]}\\r\\n                        name=\\\"commune\\\"\\r\\n                      >\\r\\n                        {userInfo.data.commune ? (\\r\\n                          <Select\\r\\n                            onChange={(value) => {\\r\\n                              setCommuneID(value);\\r\\n                            }}\\r\\n                            placeholder=\\\"Phường/xã\\\"\\r\\n                            className=\\\"select-payment-cart\\\"\\r\\n                          >\\r\\n                            {renderCommune}\\r\\n                          </Select>\\r\\n                        ) : (\\r\\n                          <Select\\r\\n                            onChange={(value) => {\\r\\n                              setCommuneID(value);\\r\\n                            }}\\r\\n                            placeholder=\\\"Phường/xã\\\"\\r\\n                            className=\\\"select-payment-cart\\\"\\r\\n                          >\\r\\n                            {renderCommune}\\r\\n                          </Select>\\r\\n                        )}\\r\\n                      </Form.Item>\\r\\n                    </S.FlexItemSelect>\\r\\n                  </S.FromFlexSelect>\\r\\n                  <Form.Item name=\\\"note\\\">\\r\\n                    <Input\\r\\n                      className=\\\"input-payment-cart\\\"\\r\\n                      placeholder=\\\"Ghi chú (ví dụ: giao giờ hành chính)\\\"\\r\\n                    />\\r\\n                  </Form.Item>\\r\\n                  <S.InfoTitle>Hình thức thanh toán</S.InfoTitle>\\r\\n                  <Form.Item\\r\\n                    styles={{\\r\\n                      width: \\\"100%\\\",\\r\\n                    }}\\r\\n                  >\\r\\n                    <Radio.Group\\r\\n                      defaultValue=\\\"cash\\\"\\r\\n                