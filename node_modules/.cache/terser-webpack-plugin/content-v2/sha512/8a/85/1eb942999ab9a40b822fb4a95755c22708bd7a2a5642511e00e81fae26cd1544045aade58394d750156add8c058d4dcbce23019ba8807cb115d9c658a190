              layout=\\\"vertical\\\"\\r\\n            >\\r\\n              <Form.Item name=\\\"url\\\" label=\\\"Image Product Link\\\">\\r\\n                <Input className=\\\"input-admin\\\" />\\r\\n              </Form.Item>\\r\\n              <Form.Item name=\\\"color\\\" label=\\\"Color\\\">\\r\\n                <Select>\\r\\n                  <Option value=\\\"black\\\">Black </Option>\\r\\n                  <Option value=\\\"white\\\"> White</Option>\\r\\n                  <Option value=\\\"navyblue\\\">Navy Blue</Option>\\r\\n                  <Option value=\\\"darkblue\\\">Dark Blue</Option>\\r\\n                  <Option value=\\\"aquablue\\\">Aqua Blue</Option>\\r\\n                  <Option value=\\\"lightblue\\\">Light Blue</Option>\\r\\n                  <Option value=\\\"beachblue\\\">Beach Blue </Option>\\r\\n                  <Option value=\\\"green\\\"> Green</Option>\\r\\n                </Select>\\r\\n              </Form.Item>\\r\\n              <Form.Item>\\r\\n                <Button\\r\\n                  style={{\\r\\n                    backgroundColor: \\\"#e7dbce\\\",\\r\\n                    borderRadius: \\\"5px\\\",\\r\\n                  }}\\r\\n                  htmlType=\\\"submit\\\"\\r\\n                >\\r\\n                  Upload Image Link\\r\\n                </Button>\\r\\n              </Form.Item>\\r\\n            </Form>\\r\\n          </S.FormBody>\\r\\n          <S.FormBody>\\r\\n            <Form\\r\\n              onFinish={(values) => handleCreateColor(values)}\\r\\n              initialValues={initialValues3}\\r\\n              labelCol={{\\r\\n                span: 12,\\r\\n              }}\\r\\n              wrapperCol={{\\r\\n                span: 16,\\r\\n              }}\\r\\n              layout=\\\"vertical\\\"\\r\\n            >\\r\\n              <Form.Item name=\\\"url\\\" label=\\\"Product color link\\\">\\r\\n                <Input className=\\\"input-admin\\\" />\\r\\n              </Form.Item>\\r\