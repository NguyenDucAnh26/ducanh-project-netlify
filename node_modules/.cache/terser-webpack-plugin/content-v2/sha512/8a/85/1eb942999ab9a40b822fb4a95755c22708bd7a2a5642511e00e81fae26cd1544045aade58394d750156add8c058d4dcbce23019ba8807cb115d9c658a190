       setProvinceName(\\r\\n                        province.find((item) => item.idProvince === value).name\\r\\n                      );\\r\\n                      setProvinceID(value);\\r\\n                    }}\\r\\n                  >\\r\\n                    {renderProvince}\\r\\n                  </Select>\\r\\n                </Form.Item>\\r\\n              </S.FlexItemSelect>\\r\\n              <S.FlexItemSelect>\\r\\n                <Form.Item name=\\\"district\\\">\\r\\n                  <Select\\r\\n                    onChange={(value) => {\\r\\n                      setDistrictName(\\r\\n                        district.find((item) => item.idDistrict === value).name\\r\\n                      );\\r\\n                      setDistrictID(value);\\r\\n                    }}\\r\\n                    placeholder=\\\"Quận/huyện\\\"\\r\\n                    className=\\\"select-payment-cart\\\"\\r\\n                  >\\r\\n                    {renderDistrict}\\r\\n                  </Select>\\r\\n                </Form.Item>\\r\\n              </S.FlexItemSelect>\\r\\n              <S.FlexItemSelect>\\r\\n                <Form.Item name=\\\"commune\\\">\\r\\n                  <Select\\r\\n                    onChange={(value) => {\\r\\n                      setCommuneName(\\r\\n                        commune.find((item) => item.idCommune === value).name\\r\\n                      );\\r\\n                    }}\\r\\n                    placeholder=\\\"Phường/xã\\\"\\r\\n                    className=\\\"select-payment-cart\\\"\\r\\n                  >\\r\\n                    {renderCommune}\\r\\n                  </Select>\\r\\n                </Form.Item>\\r\\n              </S.FlexItemSelect>\\r\\n            </S.FromFlexSelect>\\r\\n            <Form.Item\\r\\n              styles={{\\r\\n                width: \\\"100%\\\",\\r\\n              }}\\r\\n            >\\r\\n              <S.BtnWrap>\\r\\n                <Button\\r\\n                  classN