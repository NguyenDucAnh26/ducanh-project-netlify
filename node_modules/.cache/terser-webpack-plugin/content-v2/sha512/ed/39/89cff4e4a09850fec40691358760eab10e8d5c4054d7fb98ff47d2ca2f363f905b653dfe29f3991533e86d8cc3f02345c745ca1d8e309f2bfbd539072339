gate(ROUTES.ADMIN.PRODUCTS);\\r\\n            }}\\r\\n          >\\r\\n            Products page\\r\\n          </S.AddProduct>\\r\\n        </S.HeadContainer>\\r\\n        <S.BodyWrapper>\\r\\n          <S.FormBody>\\r\\n            <Form\\r\\n              layout=\\\"vertical\\\"\\r\\n              name=\\\"Add Variant\\\"\\r\\n              onFinish={(values) => handleCreateProduct(values)}\\r\\n              initialValues={initialValues}\\r\\n              labelCol={{\\r\\n                span: 12,\\r\\n              }}\\r\\n              wrapperCol={{\\r\\n                span: 16,\\r\\n              }}\\r\\n            >\\r\\n              <Form.Item name=\\\"color\\\" label=\\\"Color\\\">\\r\\n                <Select>\\r\\n                  <Option value=\\\"black\\\">Black </Option>\\r\\n                  <Option value=\\\"white\\\"> White</Option>\\r\\n                  <Option value=\\\"navyblue\\\">Navy Blue</Option>\\r\\n                </Select>\\r\\n              </Form.Item>\\r\\n              <Form.Item name=\\\"size\\\" label=\\\"Size\\\">\\r\\n                <Select>\\r\\n                  <Option value=\\\"s\\\">S </Option>\\r\\n                  <Option value=\\\"m\\\"> M</Option>\\r\\n                  <Option value=\\\"l\\\">L </Option>\\r\\n                </Select>\\r\\n              </Form.Item>\\r\\n              <Form.Item name=\\\"quantity\\\" label=\\\"Quantity\\\">\\r\\n                <InputNumber />\\r\\n              </Form.Item>\\r\\n              <Form.Item>\\r\\n                <Button type=\\\"primary\\\" htmlType=\\\"submit\\\">\\r\\n                  Complete\\r\\n                </Button>\\r\\n              </Form.Item>\\r\\n            </Form>\\r\\n          </S.FormBody>\\r\\n          <S.FormBody>\\r\\n            <Form\\r\\n              onFinish={(values) => handleCreateImageProduct(values)}\\r\\n              initialValues={initialValues2}\\r\\n              labelCol={{\\r\\n                span: 12,\\r\\n              }}\\r\\n              wrapperCol={{\\r\\n                span: 16,\\r\\n              }}\\r\\n              layout=\\\"vertical\\\"\\r\\n            >\\r\\n              <Form.Item name=\\\"url\\\" label=\\\"Image Product Link\\\">\\r\\n                <Input />\\r\\n              </Form.Item>\\r\\n              <Form.Item name=\\\"color\\\" label=\\\"Color\\\">\\r\\n                <Select>\\r\\n                  <Option value=\\\"black\\\">Black </Option>\\r\\n                  <Option value=\\\"white\\\"> White</Option>\\r\\n                  <Option value=\\\"navyblue\\\">Navy Blue</Option>\\r\\n                  <Option value=\\\"darkblue\\\">Dark Blue</Option>\\r\\n                  <Option value=\\\"aquablue\\\">Aqua Blue</Option>\\r\\n                  <Option value=\\\"lightblue\\\">Light Blue</Option>\\r\\n                  <Option value=\\\"beachblue\\\">Beach Blue </Option>\\r\\n                  <Option value=\\\"green\\\"> Green</Option>\\r\\n                </Select>\\r\\n              </Form.Item>\\r\\n              <Form.Item>\\r\\n                <Button type=\\\"primary\\\" htmlType=\\\"submit\\\">\\r\\n                  Upload Image Link\\r\\n                </Button>\\r\\n              </Form.Item>\\r\\n            </Form>\\r\\n          </S.FormBody>\\r\\n          <S.FormBody>\\r\\n            <Form\\r\\n              onFinish={(values) => handleCreateColor(values)}\\r\\n              initialValues={initialValues3}\\r\\n              labelCol={{\\r\\n                span: 12,\\r\\n              }}\\r\\n              wrapperCol={{\\r\\n                span: 16,\\r\\n              }}\\r\\n              layout=\\\"vertical\\\"\\r\\n            >\\r\\n              <Form.Item name=\\\"url\\\" label=\\\"Product color link\\\">\\r\\n                <Input />\\r\\n              </Form.Item>\\r\\n              <Form.Item name=\\\"color\\\" label=\\\"Color\\\">\\r\\n                <Select>\\r\\n                  <Option value=\\\"black\\\">Black </Opt