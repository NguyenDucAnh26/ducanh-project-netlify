                    overflow: \\\"hidden\\\",\\r\\n                      }}\\r\\n                      onChange={(value) => {\\r\\n                        setAmountSelected(value);\\r\\n                      }}\\r\\n                      defaultValue={1}\\r\\n                    />\\r\\n                    <Button\\r\\n                      disabled={sizeSelected === \\\"\\\" ? true : false}\\r\\n                      onClick={() => handleAddProduct()}\\r\\n                      className=\\\"btn\\\"\\r\\n                      icon={<ShoppingOutlined />}\\r\\n                      style={{\\r\\n                        flex: 1,\\r\\n                        backgroundColor: \\\"black\\\",\\r\\n                        color: \\\"white\\\",\\r\\n                        marginLeft: \\\"20px\\\",\\r\\n                        borderRadius: \\\"20px\\\",\\r\\n                        height: \\\"40px\\\",\\r\\n                      }}\\r\\n                    >\\r\\n                      <span style={{ color: \\\"white\\\" }}>\\r\\n                        {sizeSelected === \\\"\\\"\\r\\n                          ? \\\"Chọn biến thể\\\"\\r\\n                          : \\\"Thêm vào giỏ hàng\\\"}\\r\\n                      </span>\\r\\n                    </Button>\\r\\n                  </S.GridRow>\\r\\n                </Form.Item>\\r\\n              </Form>\\r\\n            </S.FormProduct>\\r\\n            <S.BonusContent>\\r\\n              <Row gutter={[16, 24]}>\\r\\n                <Col lg={8} sm={8} xs={8}>\\r\\n                  <S.BonusWrapper>\\r\\n                    <S.BonusIcon>\\r\\n                      <S.IconImg\\r\\n                        src=\\\"https://www.coolmate.me/images/icons/icon3.svg\\\"\\r\\n                        alt=\\\"Coolmate\\\"\\r\\n                      />\\r\\n                    </S.BonusIcon>\\r\\n                    <S.BonusDes>\\r\\n                      Đổi trả cực dễ\\r\\n                      <br />\\r\\n                      chỉ